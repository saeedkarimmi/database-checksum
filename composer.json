{
  "name": "saeedkarimmi/database-checksum",
  "description": "A database checksum",
  "license": "MIT",
  "keywords": [
    "database",
    "checksum"
  ],
  "type": "library",
  "authors": [
    {
      "name": "saeed karimi",
      "email": "saeedkarimi454@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0"
  },
  "require-dev": {
    "illuminate/database": "^5.2|^6.0|^7.0",
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "saeedkarimmi\\DatabaseChecksum\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "saeedkarimmi\\DatabaseChecksum\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "saeedkarimmi\\DatabaseChecksum\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
